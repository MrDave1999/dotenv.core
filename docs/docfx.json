{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "src/**.csproj"
          ]
        },
        {
          "src": "..",
          "files": [
            "plugins/Microsoft.Extensions.DI/src/**.csproj"
          ]
        },
        {
          "src": "..",
          "files": [
            "plugins/Microsoft.Extensions.Config/src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",  
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "extensions/**.md",
          "extensions/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "DotEnv.Core",
      "_enableNewTab": true,
      "_appFaviconPath": "images/dotenv-icon.ico",
      "_appFooter": "Copyright Â©2021 <b>DotEnv.Core</b>",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/MrDave1999/dotenv.core",
        "branch": "master"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "plugins/memberpage/content"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xref": [ "xrefmap.json" ]
  }
}